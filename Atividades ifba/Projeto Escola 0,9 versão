#include <iostream>
#include <stdio.h>
#include <string.h>
#include<conio.h>

	
float CadastroAlunos( char nomes[11][11], char nas[11][11],char sex[10],int mat[10],int cpf[10])
{
	

	int N,j,i,k=0,k1=0,k2=0;
    char aux[11];
    int nmat;
    char let,ret;
    char let1,ret1;
    char let2,ret2;
     int r3;
     
     let=ret;
     let1=ret1;
     let2=ret2;
     
     int rep,op,l,final;
     int anv[50],at;
     
	
	printf("\n Digite o total de alunos matriculados:\n");
	scanf("%d",&N);
	getchar();
	
	printf("Digite o mes atual:\n");
    scanf("%d",&at);
	

	 
	 
	for(i=1;i<=N;i=i+1)
	{
		
		printf("\n Digite o  nome do aluno :\n");
		scanf("%[^\n]s",nomes[i]);
		getchar();
		
		
		
		printf("Digite a data de nascimento:\n");
		scanf("%[^\n]s",nas[i]);
		getchar();
		
		printf("Digite o sexo:\n");
		scanf("%c",&sex[i]);
		getchar();
		
		printf("Digite a Matricula:\n");
		scanf("%d",&mat[i]);
		getchar();
		
		printf("Digite o seu cpf:\n");
		scanf("%d",&cpf[i]);
		getchar();
		
		printf("Digite o mes do seu aniversario:\n");
        scanf("%d",&anv[i]);  
        getchar();
        
        if(anv[i]==at)
{
	printf(" \n Aniversariante do mes:\n");
}
		
	}
	
	for(i=1;i<=N;i=i+1)
{
	if(anv[i]==at)
{
	printf(" \n Aniversariantentes do mes:\n");
	
	printf(" \n %s \n ",nomes[i]);
}
	
}

	for(i=0;i<=N;i=i+1)
	{
	
	 if(sex[i]=='m')
		{
			printf("\n Os alunos do sexo masculino:\n");
			
			printf("\n %s\n",nomes[i]);
		}
}

	
	for(i=0;i<=N;i=i+1)
	{
		if(sex[i]=='f'){
		
		
			printf("\n Os alunos do sexo feminino :\n");
			
			printf(" \n %s: \n",nomes[i]);
		}
		
		
	}
	

printf("\naqui est達o os nomes na ordem digitados\n");
	for(i=0;i<=N;i++){
		printf("%s\n",nomes[i]);
	}
	for(i=0;i<=N;i++){
        for(j=i+1;j<=N;j++){
            if( strcoll(nomes[i],nomes[j])>0){
                strcpy(aux,nomes[i]);
                strcpy(nomes[i],nomes[j]);
                strcpy(nomes[j],aux);
            }
        }
	}
	printf("\naqui os nomes em ordem crescente\n");
	for(i=0;i<=N;i++){
		printf("%s\n",nomes[i]);
	}
	


printf("\naqui est達o os nomes na ordem digitados com base no nascimento\n");
	for(i=0;i<=N;i++){
		printf("%s\n",nas[i]);
	}
	for(i=1;i<=N;i++){
        for(j=i+1;j<=N;j++){
            if( strcoll(nas[i],nas[j])>0){
                strcpy(aux,nas[i]);
                strcpy(nas[i],nas[j]);
                strcpy(nas[j],aux);
            }
        }
	}
	printf("\naqui matriculas em ordem crescente  e seu nome ordem crescente\n");
	for(i=0;i<=N;i++){
		printf("\n %s\n \n %s\n ",nas[i],nomes[i]);
	}
	
	for(i=0;i<=N;i++){
	for(i=0;i<=N;i++){
            if(nomes[i][i]==ret){
            	
            	k=k+1;
               
            }
        if(k>=1)
        {
        	break;
		}
	}
	// printf("A quantidade que se repete eh: %d ",k);
	for(i=0;i<=N;i++){
            if(nomes[i][i]==ret1){
            	
            	k1=k1+1;
               
            }
        if(k1>=1)
        {
        	break;
		}
	}
	// printf("A quantidade que se repete eh: %d ",k1);
	 
	for(i=0;i<=N;i++){
            if(nomes[i][i]==ret2){
            	
            	k2=k2+1;
               
            }
        if(k2>=1)
        {
        	break;
		}
	}
	// printf("A quantidade que se repete eh: %d ",k2);
	
	r3=k+k1+k2;
	
	if(r3==3)
	{
		
		printf(" \n As tres letras foram achadas no nome: %s\n ",nomes[i]);
	}

}


printf("Digite 1 para efetuar alguma alteracao relacionada a exclusao ou troca ou 2 para finalizar o programa:\n");
scanf("%d",&final);

if(final==1)
{

printf("Se voce deseja substutuir digite 0 para aluno/ se voce deseja apagar digite 1  para aluno :\n");
scanf("%d",&rep);
getchar();



printf("Digite a quantidade de vezes que pretende efetuar a operacao:\n");
scanf("%d",&op);

getchar();

for(i=0;i<=op;i=i+1)
{


if(rep==0)
{
	printf("Digite o local da substituicao:\n ");
	scanf("%d",&l);  // ou &i
	getchar();
	
	printf("Reinsira novamente os dados:\n");
	
	printf("\n Digite o  nome do aluno :\n");
		scanf("%[^\n]s",nomes[l]);
		getchar();
		
		
		
		printf("Digite a data de nascimento:\n");
		scanf("%[^\n]s",nas[l]);
		getchar();
		
		printf("Digite o sexo:\n");
		scanf("%c",&sex[l]);
		getchar();
		
		printf("Digite a Matricula:\n");
		scanf("%d",&mat[l]);
		getchar();
		
		printf("Digite o seu cpf:\n");
		scanf("%d",&cpf[l]);
		getchar();
	
}

else if(rep==1)
{
	printf("Digite o local que sera apagado:\n ");
	scanf("%d",&l);
	
	// printf("Nome");
	// scanf("nome[l]");
	
	  //nome[l]==0;
	  
	  
	printf("\n Digite o  nome do aluno :\n");
		scanf("%[^\n]s",nomes[l]);
		getchar();
		
		nomes[l]==0;
		
		printf("Digite a data de nascimento:\n");
		scanf("%[^\n]s",nas[l]);
		getchar();
		
		nas[l]==0;
		
		printf("Digite o sexo:\n");
		scanf("%c",&sex[l]);
		getchar();
		
		sex[l]==0;
		
		printf("Digite a Matricula:\n");
		scanf("%d",&mat[l]);
		getchar();
		
		mat[l]==0;
		
		printf("Digite o seu cpf:\n");
		scanf("%d",&cpf[l]);
		getchar();
		
		cpf[l]==0;
}

}


for(i=0;i<=N;i=i+1)
{
	if(nomes[i]==0)
	{
		
		printf("O nome correspondente a o numero:%d foi excluido do sistema",i);
		
		i=i+1;
		
		for(i=1;i<=nmat;i=i+1)
		{
			printf("Os atuais nomes registrados nos sistemas sao:%s",nomes[i]);
		}
	}
}

}
else if(final==1||final!=0)
{
	printf("Fim do programa :\n");
}

	
	return N;
}


float CadastroProfessores( char nomes1[11][11], char nas1[11][11],char sex1[10],int mat1[10],int cpf1[10])
{
	

	int N1,j,i,k=0,k1=0,k2=0;
    char aux1[11];
    int nmat;
    
    char let,ret;
    char let1,ret1;
    char let2,ret2;
     int r3;
     
     let=ret;
     let1=ret1;
     let2=ret2;
     
     int rep,op,l,final1;
     
    int anv1[50],at1;
	
	printf("Digite o total de professores matriculados:\n");
	scanf("%d",&N1);
	getchar();
	
	
printf("Digite o mes atual:\n");
scanf("%d",&at1);
getchar();

	 
	 
	for(i=1;i<=N1;i=i+1)
	{
		
		printf("\n Digite o  nome do Professor :\n");
